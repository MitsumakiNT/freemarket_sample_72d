//お問い合わせのその他
= render "items/header"
.mypage
  .container
    %main.main-container
      = render partial: "show_sidebar", locals: {}
      .main
        .main__top
        .other_enquiry_contact
          %p.other_enquiry_contact__title その他
          .other_enquiry_contact__content
            .other_enquiry_contact__content--box
              .description
                .description__box
                  %h1.title お問い合わせの内容
                  %p.required 必須
                %input.description__content{:type => "text", :placeholder => "例:本物のトラクターを頼んだのにミニカーのトラクターが届いた"}
              .form
                = link_to "#" do
                  %input.submit.btn{:type => "submit", :value => "送信する"}
